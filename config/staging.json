{
	"apiLimiter": {
		"sensitivity": {
			"veryLow": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"low": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"medium": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"high": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"critical": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			}
		}
	},
	"expressPort": 5000,
	"mongoose": {
		"retrySettings": {
			"maxAttempts": 5,
			"interval": 5000
		},
		"options": {
			"retryWrites": "true",
			"w": "majority",
			"appName": "staging"
		}
	},
	"bcrypt": {
		"saltRounds": 1
	},
	"cors": {
		"origin": "https://staging.gaslight.fun",
		"credentials": true
	},
	"cookieOptions": {
		"httpOnly": true,
		"secure": true,
		"SameSite": "strict"
	},
	"session": {
		"expiry": 5184000000
	},
	"webhook": {
		"cors": {
			"origin": "*",
			"credentials": true
		}
	},
	"redis": {
		"prefix": "gaslight-backend-staging:"
	},
    "user": {
        "verificationExpiry": 86400000,
        "passwordResetExpiry": 86400000
    },
	"microservices": {
		"codeRunner": {
			"host": "code-runner-service"
		}
	},
	"codeRunnerTimeouts": {
		"executionTimeout": 1,
		"loadingTimeout": 100
	}
}
